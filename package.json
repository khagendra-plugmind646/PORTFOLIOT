{
  "name": "dopefolio",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "compile:scss": "sass sass/main.scss css/style.css -w",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.css -o css/style.css",
    "compress:css": "sass css/style.css css/style.css --output-style compressed",
    "build:css": "npm-run-all prefix:css compress:css",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "author": "",
  "license": "GPL-3.0",
  "devDependencies": {
    "autoprefixer": "^10.3.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.3.1"
  },
  "dependencies": {
    "react-scripts": "^5.0.1",
    "sass": "^1.83.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
